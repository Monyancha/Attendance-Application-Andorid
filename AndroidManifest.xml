<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jam"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        	<activity
            android:name=".EnterCourse"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.ENTERCOURSE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        	<activity
            android:name=".TakeAttendence"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.TAKEATTENDENDENCE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        	
        <activity
            android:name=".FragmentTab"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.FRAGMENTTAB" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
 <activity
            android:name=".NewTerm"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.NEWTERM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

 
  <activity
            android:name=".Help"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.jam.HELP" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
 
        	<activity
            android:name=".Splash1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        

        <activity  android:name="com.example.jam.Second" android:label="@string/app_name"></activity>
        <activity  android:name="com.example.jam.DisplayActivity" android:label="@string/app_name"></activity>
        <activity  android:name="com.example.jam.DisplayCourse" android:label="@string/app_name"></activity>
        <activity  android:name="com.example.jam.DeleteActivity" android:label="@string/app_name"></activity>
         <activity  android:name="com.example.jam.DeleteCourse" android:label="@string/app_name"></activity>
         <activity  android:name="com.example.jam.EnterDate" android:label="@string/app_name"></activity>
         <activity  android:name="com.example.jam.ViewActivity" android:label="@string/app_name"></activity>
           </application>

</manifest>
